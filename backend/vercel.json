{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "installCommand": "npm install --prefix backend && npm install --prefix ../frontend",
  "buildCommand": "npm run build --prefix backend && npm run build --prefix ../frontend"
}
