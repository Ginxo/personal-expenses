{
  "version": 2,
  "builds": [
    {
      "src": "backend/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/dist/index.js"
    }
  ],
  "buildCommand": "npm run build --prefix backend",
  "installCommand": "npm install --prefix backend"
}
