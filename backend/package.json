{
  "name": "personal-expenses-backend",
  "version": "0.0.1",
  "description": "A personal app for analysing personal expenses .",
  "repository": "https://github.com/Ginxo/personal-expenses.git",
  "homepage": "https://ginxo.github.io/personal-expenses",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "npm run prisma:generate",
    "prebuild": "npm run type-check && npm run clean",
    "build": "webpack",
    "eslint": "eslint --ext .tsx,.js ./",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "db:generate": "zenstack generate --schema ./schema.zmodel",
    "prisma:generate": "prisma generate",
    "db:push": "prisma db push --schema ./prisma/schema.prisma",
    "start": "node index.js",
    "start:dev": "tsx watch index.ts",
    "clean": "rimraf dist",
    "predeploy": "npm run build"
  },
  "devDependencies": {
    "@mockoon/cli": "^9.0.0",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@zenstackhq/openapi": "^2.10.0",
    "@zenstackhq/runtime": "2.10.0",
    "@zenstackhq/server": "^2.10.0",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.0",
    "prop-types": "^15.8.1",
    "swagger-ui-express": "^5.0.1",
    "ts-loader": "^9.5.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0",
    "zenstack": "2.10.0"
  },
  "dependencies": {
    "@prisma/client": "6.0.x",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "prisma": "6.0.x",
    "qs": "^6.13.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "engines": {
    "node": "<=20.18.1"
  }
}
